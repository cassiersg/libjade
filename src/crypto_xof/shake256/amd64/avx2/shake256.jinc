from Jade require "common/keccak/keccak1600/amd64/avx2/keccak1600.jinc"

inline fn __shake256_avx2(reg u64 out outlen in inlen, reg u64 ms) -> #msf reg u64
{
  reg u64 rate;
  reg u8 trail_byte;

  trail_byte = 0x1F;
  rate = (1088/8);

  ms = __keccak1600_avx2(out, outlen, in, inlen, trail_byte, rate, ms);
  return ms;
}
